component:airtimeGroup:airtimeMonday:true
component:airtimeGroup:airtimeTuesday:false
component:airtimeGroup:airtimeWednesday:false
component:airtimeGroup:airtimeThursday:false
component:airtimeGroup:airtimeFriday:false
