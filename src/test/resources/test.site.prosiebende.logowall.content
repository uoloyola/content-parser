# defining a test prosieben site
# this is a comment
id:.nombre
inputtemplate:
securityparent:p.siteengine.Sites.d
name:Logowall Test Site
component:advanced/atselected:value:true
component:advanced/chselected:value:true
component:advanced/deselected:value:true
component:editableBySite:value:de
component:moduleMetadata/breadcrumbDefaultH1Message:value:Test Home
component:showinhtmlsitemap:value:false
ref:pageLayout:selected:np.it.FlexiblePageLayout
list:rowlist:.logowallrow

# defining a test formatpage
id:.sendungen.logowall.formatpage
inputtemplate:p.siteengine.Page
securityparent:np.ct.prosieben.de.sendungen.d
name:Logowall Test Format Page
component:advanced/atselected:value:true
component:advanced/chselected:value:true
component:advanced/deselected:value:true
component:editableBySite:value:de
component:moduleMetadata/breadcrumbDefaultH1Message:value:Logowall Test Format Page
component:showinhtmlsitemap:value:false
ref:pageLayout:selected:np.it.Flexible2ColumnPageLayout
publish:np.ct.prosieben.de.sendungen.d
list:singleBroadcastHighlightList:.broadcasthighlight.formatpage.logowalltest
list:singleInternalLinkList:.sendungen.logowall.videoplayerpage

# defining broadcasthighlight elements
id:.broadcasthighlight.formatpage.logowalltest
inputtemplate:np.it.module.broadcast.Highlight
securityparent:.sendungen.logowall.formatpage
name: Test Logowall Formatpage Broadcasthighlight
